@model IEnumerable<VRLibrary.Models.Application>

<div class="responsiveFullPageDiv">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser1.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser1.Surname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser1.Email)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUser.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUser.Surname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUser.Email)
                </td>
                <td>
                    @Html.ActionLink("Accept", "Delete", new { id = item.ApplicationId })
                </td>
            </tr>
        }

    </table>
</div>